	id INTEGER PRIMARY KEY AUTOINCREMENT,
	usuario VARCHAR(255) NOT NULL UNIQUE,
	clave VARCHAR(255),
	nombre VARCHAR(50),
	apellido VARCHAR(50),
	fecha_ingreso DATETIME DEFAULT (datetime('now')),
	fecha_actualizado DATETIME DEFAULT (datetime('now'))
);

CREATE TABLE colaboradores(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nombre VARCHAR(50),
	apellido VARCHAR(50),
	identificacion VARCHAR(13) NOT NULL UNIQUE
);

CREATE TABLE especies(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nombre VARCHAR(50)
);

CREATE TABLE razas(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nombre VARCHAR(50),
	especie_id INTEGER,
	FOREIGN KEY (especie_id) REFERENCES especies(id)
);

CREATE TABLE mascotas(
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nombre VARCHAR(50),
	sexo VARCHAR(20),
	pesoKg REAL,
	especie_id INTEGER,
	raza_id INTEGER,
	vacunacion_estado BOOLEAN,
	adopcion_estado BOOLEAN,
	colaborador_id INTEGER,
	fecha_ingreso DATETIME DEFAULT (datetime('now')),
	fecha_actualizado DATETIME DEFAULT (datetime('now')),
	FOREIGN KEY (especie_id) REFERENCES especies(id),
	FOREIGN KEY (raza_id) REFERENCES razas(id),
	FOREIGN KEY (colaborador_id) REFERENCES colaboradores(id)
);</sql><current_tab id="0"/></tab_sql></sqlb_project>
